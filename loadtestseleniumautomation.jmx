	   password.sendKeys(&quot;3b6ztwWN5D8LV72q&quot;)
	   java.lang.Thread.sleep(2000)

	   var login = WDS.browser.findElement(pkg.By.xpath(			   &quot;//*[@id=&apos;root&apos;]/div/div/div/div/div[2]/form/button/span[1]&quot;));
	   login.click();
	   java.lang.Thread.sleep(5000)

	   var userprofile = WDS.browser.findElement(pkg.By.xpath(
&quot;//*[@id=&apos;root&apos;]/div/section/header/div[2]/div[2]/button/span[1]/div&quot;));
	   userprofile.click();
	   java.lang.Thread.sleep(3000)


	   var logout = WDS.browser.findElement(pkg.By.xpath(
&quot;//*[@id=&apos;menu-list-grow&apos;]/li[2]/a&quot;));
